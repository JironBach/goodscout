<p id="notice"><%= notice %></p>

<h1>Listing Enginners</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Encrypted password</th>
      <th>Self introduction</th>
      <th>Living place</th>
      <th>Fb uid</th>
      <th>Github uid</th>
      <th>Age</th>
      <th>Phone number</th>
      <th>Status</th>
      <th>Job history</th>
      <th>Notes</th>
      <th>Is registration enabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enginners.each do |enginner| %>
      <tr>
        <td><%= enginner.name %></td>
        <td><%= enginner.email %></td>
        <td><%= enginner.encrypted_password %></td>
        <td><%= enginner.self_introduction %></td>
        <td><%= enginner.living_place %></td>
        <td><%= enginner.fb_uid %></td>
        <td><%= enginner.github_uid %></td>
        <td><%= enginner.age %></td>
        <td><%= enginner.phone_number %></td>
        <td><%= enginner.status %></td>
        <td><%= enginner.job_history %></td>
        <td><%= enginner.notes %></td>
        <td><%= enginner.is_registration_enabled %></td>
        <td><%= link_to 'Show', enginner %></td>
        <td><%= link_to 'Edit', edit_enginner_path(enginner) %></td>
        <td><%= link_to 'Destroy', enginner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enginner', new_enginner_path %>
